{
    "name": "gtechmx/fractal",
    "description": "This is a package to be able to use the fractal library in a better way.",
    "license": "PHP-3.01",
    "authors": [{
        "name": "Miguel Zubiate",
        "email": "mzubiate@reedexpo.com"
    }],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "~5.7.0|~5.8.0|~6.0|~6.1|~6.2|~6.3|~6.4|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Gtechmx\\Fractal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gtechmx\\Fractal\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gtechmx\\Fractal\\Providers\\FractalServiceProvider"
            ],
            "aliases": {
                "Fractal": "Gtechmx\\Fractal\\Facades\\Fractal"
            }
        }
    }
}
